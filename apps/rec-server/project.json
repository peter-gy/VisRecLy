{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/rec-server",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "make -C ./apps/rec-server build"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "make -C ./apps/rec-server serve"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "make -C ./apps/rec-server test"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "make -C ./apps/rec-server lint"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "make -C ./apps/rec-server format"
      }
    }
  },
  "tags": []
}
